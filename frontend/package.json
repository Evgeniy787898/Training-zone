{
    "name": "tzona-v2-frontend",
    "version": "2.0.0",
    "description": "TZONA V2 Frontend - Vue 3 + TypeScript",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "node ./scripts/vue-tsc-patched.cjs --noEmit && vite build && node ./scripts/cdn-headers.mjs",
        "analyze:bundle": "ANALYZE_BUNDLE=1 npm run build",
        "check:bundle": "npm run build && node ./scripts/bundle-budget.mjs",
        "build:ssr": "node ./scripts/vue-tsc-patched.cjs --noEmit && vite build --ssr src/entry-server.ts",
        "generate": "npm run build && vite build --ssr src/entry-server.ts && node ./scripts/prerender.mjs && node ./scripts/cdn-headers.mjs",
        "deploy:cdn": "node ./scripts/deploy-cdn.mjs",
        "preview": "vite preview",
        "type-check": "node ./scripts/vue-tsc-patched.cjs --noEmit",
        "test:snapshot": "vitest run --config vitest.config.ts",
        "lint": "eslint \"src/**/*.{ts,tsx,vue}\""
    },
    "dependencies": {
        "@tresjs/cientos": "^5.1.1",
        "@tresjs/core": "^5.1.1",
        "@types/three": "^0.181.0",
        "@vue/server-renderer": "^3.4.0",
        "@vueuse/core": "^10.7.0",
        "axios": "^1.6.2",
        "chart.js": "^4.5.1",
        "date-fns": "^3.0.0",
        "idb-keyval": "^6.2.2",
        "jszip": "^3.10.1",
        "pinia": "^2.1.7",
        "three": "^0.181.2",
        "vue": "^3.4.0",
        "vue-chartjs": "^5.3.3",
        "vue-router": "^4.2.5",
        "web-vitals": "^5.1.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudfront": "^3.496.0",
        "@aws-sdk/client-s3": "^3.496.0",
        "@playwright/test": "^1.57.0",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.16",
        "eslint": "^9.39.1",
        "eslint-plugin-vue": "^10.6.2",
        "happy-dom": "^20.0.10",
        "mime-types": "^2.1.35",
        "postcss": "^8.4.32",
        "rollup-plugin-visualizer": "^5.14.0",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.8",
        "vite-plugin-pwa": "^1.1.0",
        "vitest": "^4.0.9",
        "vue-tsc": "^1.8.25",
        "workbox-cacheable-response": "^7.3.0",
        "workbox-core": "^7.3.0",
        "workbox-expiration": "^7.3.0",
        "workbox-precaching": "^7.3.0",
        "workbox-routing": "^7.3.0",
        "workbox-strategies": "^7.3.0"
    }
}
